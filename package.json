{
  "name": "missionary-client",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.13.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint 'packages/**/src/**/*.{js,ts,tsx}'",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier 'packages/**/src/**/*.{json,yaml,md,js,ts,tsx}' --check",
    "prettier:fix": "prettier 'packages/**/src/**/*.{json,yaml,md,js,ts,tsx}' --write",
    "lint:style": "stylelint './packages/**/src/**/*.{tsx,ts,js}'",
    "lint:style:fix": "stylelint './packages/**/src/**/*.{tsx,ts,js}' --fix",
    "lint:all": "pnpm lint && pnpm prettier && pnpm lint:style",
    "lint:all:fix": "pnpm lint:fix && pnpm prettier:fix && pnpm lint:style:fix",
    "dev:admin": "pnpm --filter missionary-admin dev",
    "dev:app": "pnpm --filter missionary-app dev",
    "dev:lib": "pnpm --filter ui-library dev",
    "dev": "pnpm dev:admin & pnpm dev:app & pnpm dev:lib",
    "build:admin": "pnpm --filter missionary-admin build",
    "build:app": "pnpm --filter missionary-app build",
    "build:lib": "pnpm --filter ui-library build",
    "build": "pnpm build:admin & pnpm build:app & pnpm build:lib"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "next": "14.1.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.10",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.3.3"
  }
}
